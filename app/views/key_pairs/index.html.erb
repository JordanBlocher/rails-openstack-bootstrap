<h1>Listing key_pairs</h1>

<table>
  <tr>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Deleted at</th>
    <th>Deleted</th>
    <th>Name</th>
    <th>User</th>
    <th>Fingerprint</th>
    <th>Public key</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @key_pairs.each do |key_pair| %>
  <tr>
    <td><%= key_pair.created_at %></td>
    <td><%= key_pair.updated_at %></td>
    <td><%= key_pair.deleted_at %></td>
    <td><%= key_pair.deleted %></td>
    <td><%= key_pair.name %></td>
    <td><%= key_pair.user_id %></td>
    <td><%= key_pair.fingerprint %></td>
    <td><%= key_pair.public_key %></td>
    <td><%= link_to 'Show', key_pair %></td>
    <td><%= link_to 'Edit', edit_key_pair_path(key_pair) %></td>
    <td><%= link_to 'Destroy', key_pair, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Key pair', new_key_pair_path %>
