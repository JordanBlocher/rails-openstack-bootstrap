<%= form_for(@instance_action) do |f| %>
  <% if @instance_action.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@instance_action.errors.count, "error") %> prohibited this instance_action from being saved:</h2>

      <ul>
      <% @instance_action.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :created_at %><br />
    <%= f.datetime_select :created_at %>
  </div>
  <div class="field">
    <%= f.label :updated_at %><br />
    <%= f.datetime_select :updated_at %>
  </div>
  <div class="field">
    <%= f.label :deleted_at %><br />
    <%= f.datetime_select :deleted_at %>
  </div>
  <div class="field">
    <%= f.label :deleted %><br />
    <%= f.check_box :deleted %>
  </div>
  <div class="field">
    <%= f.label :action %><br />
    <%= f.text_field :action %>
  </div>
  <div class="field">
    <%= f.label :error %><br />
    <%= f.text_area :error %>
  </div>
  <div class="field">
    <%= f.label :instance_uuid %><br />
    <%= f.text_field :instance_uuid %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
