<h1>Listing sm_volumes</h1>

<table>
  <tr>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Deleted at</th>
    <th>Deleted</th>
    <th>Backend</th>
    <th>Vdi uuid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sm_volumes.each do |sm_volume| %>
  <tr>
    <td><%= sm_volume.created_at %></td>
    <td><%= sm_volume.updated_at %></td>
    <td><%= sm_volume.deleted_at %></td>
    <td><%= sm_volume.deleted %></td>
    <td><%= sm_volume.backend_id %></td>
    <td><%= sm_volume.vdi_uuid %></td>
    <td><%= link_to 'Show', sm_volume %></td>
    <td><%= link_to 'Edit', edit_sm_volume_path(sm_volume) %></td>
    <td><%= link_to 'Destroy', sm_volume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

