<h1>Listing iscsi_targets</h1>

<table>
  <tr>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Deleted at</th>
    <th>Deleted</th>
    <th>Target num</th>
    <th>Host</th>
    <th>Volume</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @iscsi_targets.each do |iscsi_target| %>
  <tr>
    <td><%= iscsi_target.created_at %></td>
    <td><%= iscsi_target.updated_at %></td>
    <td><%= iscsi_target.deleted_at %></td>
    <td><%= iscsi_target.deleted %></td>
    <td><%= iscsi_target.target_num %></td>
    <td><%= iscsi_target.host %></td>
    <td><%= iscsi_target.volume_id %></td>
    <td><%= link_to 'Show', iscsi_target %></td>
    <td><%= link_to 'Edit', edit_iscsi_target_path(iscsi_target) %></td>
    <td><%= link_to 'Destroy', iscsi_target, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

