<h1>Listing quota</h1>

<table>
  <tr>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Deleted at</th>
    <th>Deleted</th>
    <th>Project</th>
    <th>Resource</th>
    <th>Hard limit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quota.each do |quotum| %>
  <tr>
    <td><%= quotum.created_at %></td>
    <td><%= quotum.updated_at %></td>
    <td><%= quotum.deleted_at %></td>
    <td><%= quotum.deleted %></td>
    <td><%= quotum.project_id %></td>
    <td><%= quotum.resource %></td>
    <td><%= quotum.hard_limit %></td>
    <td><%= link_to 'Show', quotum %></td>
    <td><%= link_to 'Edit', edit_quotum_path(quotum) %></td>
    <td><%= link_to 'Destroy', quotum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quotum', new_quotum_path %>
