<h1>Listing volume_types</h1>

<table>
  <tr>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Deleted at</th>
    <th>Deleted</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @volume_types.each do |volume_type| %>
  <tr>
    <td><%= volume_type.created_at %></td>
    <td><%= volume_type.updated_at %></td>
    <td><%= volume_type.deleted_at %></td>
    <td><%= volume_type.deleted %></td>
    <td><%= volume_type.name %></td>
    <td><%= link_to 'Show', volume_type %></td>
    <td><%= link_to 'Edit', edit_volume_type_path(volume_type) %></td>
    <td><%= link_to 'Destroy', volume_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Volume type', new_volume_type_path %>
