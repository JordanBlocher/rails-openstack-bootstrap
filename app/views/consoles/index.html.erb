<h1>Listing consoles</h1>

<table>
  <tr>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Deleted at</th>
    <th>Deleted</th>
    <th>Instance name</th>
    <th>Instance</th>
    <th>Password</th>
    <th>Port</th>
    <th>Pool</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @consoles.each do |console| %>
  <tr>
    <td><%= console.created_at %></td>
    <td><%= console.updated_at %></td>
    <td><%= console.deleted_at %></td>
    <td><%= console.deleted %></td>
    <td><%= console.instance_name %></td>
    <td><%= console.instance_id %></td>
    <td><%= console.password %></td>
    <td><%= console.port %></td>
    <td><%= console.pool_id %></td>
    <td><%= link_to 'Show', console %></td>
    <td><%= link_to 'Edit', edit_console_path(console) %></td>
    <td><%= link_to 'Destroy', console, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Console', new_console_path %>
