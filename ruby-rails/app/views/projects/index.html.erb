<% content_for :head do %>
<%= stylesheet_link_tag 'dashboard-base.css.scss' -%>
<% end %>


<%= content_tag :div, :class => "dashboard" do -%>

    <%= image_tag("keys.jpg", :alt=>"")%>
        <%= content_tag :div, :class => "container" do -%>
        <%= content_tag :div, :class => "dashboard-caption" do -%>
        <h1> Projects </h1>
        <% end %>
        <% end %>
    <% end %>


<%= content_tag :div, :class => "container-fluid" do -%>
<%= content_tag :div, :class => "row-fluid" do -%>

<%= content_tag :div, :class => "span2" do -%>

<%= button_to "Projects", projects_path, :method => "get", :class => "btn btn-primary disabled"%>
<%= button_to "Services", services_path, :method => "get", :class => "btn btn-primary"%>
<%= button_to "Images", s3_images_path, :method => "get", :class => "btn btn-primary"%>
<%= button_to "Compute Nodes", compute_nodes_path, :method => "get", :class => "btn btn-primary"%>
<%= button_to "Snapshots", snapshots_path, :method => "get", :class => "btn btn-primary"%>

<% end %>

<%= content_tag :div, :class => "span10" do -%>

<%= render 'projects/default' %>

<% end %>
<% end %>
<% end %>
